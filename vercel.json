{
  "version": 2,
  "builds": [
    {
      "src": "api/main.go",
      "use": "@vercel/go"
    },
    {
      "src": "./next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/main.go"
    },
    {
      "src": "/(.*)",
      "dest": "./$1"
    }
  ]
}
